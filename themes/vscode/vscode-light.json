      "keyword.module": {
        "color": "palette.purple"
      },
      "string": { "color": "palette.orange" },
      "literal": { "color": "palette.orange" },
      "character": { "color": "palette.orange" },
      "number": { "color": "palette.green" },
      "integer": { "color": "palette.green" },
      "float": { "color": "palette.green" },
      "bool": { "color": "palette.blue" },
      "regexp": { "color": "palette.red" },
      "color": { "color": "palette.orange" },
      "keyword": { "color": "palette.purple" },
      "self": { "color": "palette.blue" },
