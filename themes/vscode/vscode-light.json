{
  "name": "VSCode Light Modern",
  "type": "light",
  "data": {
    "palette": {
      "red": "#E50000",
      "yellow": "#795E26",
      "peach": "#F1C531",
      "green": "#B5CEA8",
      "blue": "#001080",
      "gray": "#808080",
      "purple": "#AF00DB",

      "black": "#212227",
      "darkGreen": "#008000",
      "orange": "#A31515",
      "teal": "#267F99",
      "darkBlue": "#0000FF",
      "lightBlue": "#0070C1",

      "accent": "#007DD1",
      "maroon": "#800000",

      "border": "#D1D1D1",
