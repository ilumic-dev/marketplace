    "ui": {
      "editor.cursor": "palette.black",
      "editor.lineHighlight": "palette.surface",
      "selectionBackground": "#A6D8FD",
      "accent": "palette.accent",
      "editor.lineNumbers": "#6D7680",
      "editor.activeLineNumbers": "#011D81",
      "text": "#3B3B3B",
      "editor.bracketPairColorization1": "#0044F5",
      "editor.bracketPairColorization2": "#1F923A",
      "editor.bracketPairColorization3": "#803617",
      "editor.bracketPairColorization4": "#0044F5",
      "editor.bracketPairColorization5": "#1F923A",
      "editor.bracketPairColorization6": "#803617"
    },
    "editor": {
      "comment": {
        "color": "palette.darkGreen"
      },
      "lineComment": {
        "color": "palette.darkGreen"
      },
      "blockComment": {
        "color": "palette.darkGreen"
      },
      "docComment": {
        "color": "palette.darkGreen"
      },
      "tagName": {
        "color": "palette.maroon"
      },
      "phpDelimiter": {
        "color": "palette.darkBlue"
      },
      "constantName": {
        "color": "palette.lightBlue",
        "fontWeight": "bold",
        "italic": true
      },
      "angleBracket": {
