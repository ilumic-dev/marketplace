      "attributeName": {
        "color": "palette.red"
      },
      "attributeValue": {
        "color": "palette.maroon"
      },
      "variableName": {
        "color": "palette.lightBlue"
      },
      "propertyName": {
        "color": "palette.lightBlue"
      },
      "propertyDefinition": {
        "color": "palette.lightBlue"
      },
      "variableNameFunction": {
        "color": "palette.yellow"
      },
      "propertyFunction": {
        "color": "palette.yellow"
      },
      "variableNameDefinition": {
        "color": "palette.lightBlue"
      },
      "keyword.definition": {
        "color": "palette.darkBlue"
      },
      "keyword.operator": {
        "color": "palette.blue"
      },
      "keyword.control": {
        "color": "palette.purple"
